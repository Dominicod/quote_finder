
<nav class="navbar">
    <div class="logo-container">
        <span><%= image_tag("Quote-512.png", class: "quote") %>
          <h1>Quote Finder</h1>
        </span>
    </div>
</nav>
<div class="search-quotes">
    <p class="subtitle">Find The Perfect Quote!</p>
    <%= form_with url: '/quotes', local: true, method: 'get' do |f|%>
        <%= f.text_field :query, placeholder: "keyword" %>
        <%= f.submit "Search for Quotes" %>
    <% end %>
</div>